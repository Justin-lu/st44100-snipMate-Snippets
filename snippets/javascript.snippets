
#function funcName(arg){}
snippet func
	function ${1:function_name}(${2:argument}) 
	{
		${3:// body...}
	}

# var name = function(){}
snippet function
	var ${1:funciton_name} = function(${2:argument}) 
	{
		${3:// body...}
	};

# Anonymous Function
snippet f
	function(${1}) {${2}};

# if
snippet if
	if (${1:true}) {${2}}

# if ... else
snippet ife
	if (${1:true}) {${2}}
	else{${3}}
# tertiary conditional
snippet t
	${1:/* condition */} ? ${2:a} : ${3:b}
# switch
snippet switch
	switch(${1:expression}) {
		case '${3:case}':
			${4:// code}
			break;
		${5}
		default:
			${2:// code}
	}
# case
snippet case
	case '${1:case}':
		${2:// code}
		break;
	${3}
# for (...) {...}
snippet for
	for (var ${2:i} = 0; $2 < ${1:Things}.length; $2${3:++}) {
		${4:$1[$2]}
	}
# for (...) {...} (Improved Native For-Loop)
snippet forr
	for (var ${2:i} = ${1:Things}.length - 1; $2 >= 0; $2${3:--}) {
		${4:$1[$2]}
	}
# ForEach for (...) {...} (Improved Native For-Loop)
snippet fore
	for (var ${2:i} in ${1:Things}) {
		if ($1.hasOwnProperty($2)) {
			${3:$1[$2]}
		}
	}

